# SupplyChainLens Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:3001/api
VITE_ML_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:3001

# Application Configuration
VITE_APP_NAME=SupplyChainLens
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Satellite-powered ESG monitoring platform

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false
VITE_ENABLE_DEVTOOLS=false

# External Services
VITE_SENTINEL_HUB_URL=https://services.sentinel-hub.com
VITE_PLANET_API_URL=https://api.planet.com
VITE_GOOGLE_EARTH_ENGINE_URL=https://earthengine.googleapis.com

# Map Configuration
VITE_MAPBOX_TOKEN=your_mapbox_token_here
VITE_DEFAULT_MAP_CENTER_LAT=0
VITE_DEFAULT_MAP_CENTER_LNG=0
VITE_DEFAULT_MAP_ZOOM=2

# Monitoring
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
